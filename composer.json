{
    "name": "chrismou/php-irc-text-formatting",
    "description": "PHP library for adding color and styling to IRC text output",
    "keywords": [ "irc", "color", "style", "library" ],
    "homepage": "https://github.com/chrismou/php-irc-text-formatting",
    "license": "BSD-2-Clause",
    "support": {
        "email": "phergie@mou.me.uk",
        "issues": "https://github.com/chrismou/php-irc-text-formatting/issues",
        "source": "https://github.com/chrismou/php-irc-text-formatting"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*",
        "phake/phake": "2.0.0-beta2",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Chrismou\\Irc\\TextFormatting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chrismou\\Irc\\TextFormatting\\Tests\\": "tests"
        }
    }
}
